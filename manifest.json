{
"manifest_version":2,
"name": "chat_slang_expander",
 "short_name":"chatSlangExpander", 
  "version": "0.1",
  "description" : "expands slang in chat",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*",
        "http://www.facebook.com/*"
      ],
	  "js" : [
    "jquery.js",
    "list.js",
    "script.js"
    ],
      "run_at": "document_end"
    }
  ], 
  "browser_action": {
          "default_icon": "icon128.png"
   },
  "background": {
    "persistent": false,
	 "scripts": ["background.js"]
  }
}